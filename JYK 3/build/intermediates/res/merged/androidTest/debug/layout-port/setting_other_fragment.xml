<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_bg"
    android:orientation="vertical" >


    <TextView
        android:id="@+id/txt_setting_clear_rush_water"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:layout_marginTop="@dimen/setting_margin_top"
        android:text="@string/setting_clear_rush_water" />

    <TextView
        android:id="@+id/txt_setting_clear_trans_water"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/setting_clear_trans_water" />

    <TextView
        android:id="@+id/txt_clear_script_execution_result_of_the_water"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/clear_script_execution_result_of_the_water" />

    <TextView
        android:id="@+id/txt_clear_settlement_marks"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/clear_settlement_marks" />

    <TextView
        android:id="@+id/txt_clear_request_processing"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/clear_request_processing" />

    <TextView
        android:id="@+id/txt_setting_other_down_load"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/setting_other_down_load" />

    <TextView
        android:id="@+id/txt_setting_other_elecsign"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/setting_other_elecsign" />

    <com.newland.payment.ui.view.SettingSwitchView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" 
        android:visibility="gone"/>
    
    <com.newland.payment.ui.view.SettingSwitchView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />
    <com.newland.payment.ui.view.SettingSwitchView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:src="#d7d7d7" />

</LinearLayout>