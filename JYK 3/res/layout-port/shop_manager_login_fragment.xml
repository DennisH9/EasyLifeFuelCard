<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_bg"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/txt_reset_manager_password"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        	android:layout_marginTop="@dimen/setting_margin_top"
        android:text="@string/common_reset_manager_password" />

    <TextView
        android:id="@+id/txt_reset_pos_params"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/common_reset_pos_params" />

    <TextView
        android:id="@+id/txt_reset_flag"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/common_reset_flag" />
<!--  
    <TextView
        android:id="@+id/txt_clear_all_key"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/clear_all_key" />
-->
   <TextView
        android:id="@+id/txt_more_version_flag"
        style="@style/system_settings_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_item_height_91"
        android:text="@string/common_more_version_flag" />
   
	<ImageView 
	    android:layout_width="match_parent"
	    android:layout_height="@dimen/dp1"
	    android:src="#d7d7d7"/>
</LinearLayout>